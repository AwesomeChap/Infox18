<% include adminPartials/header.ejs %>

<h3><%=eventName%></h3>

<table class="responsive-table">
    <thead>
    <tr>
        <th>S. No.</th>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>College</th>
        <th>Degree</th>
        <th>Branch</th>
        <th>Year</th>
        <th>Referrer</th>
    </tr>
    </thead>
    <tbody>
    <%for(i=0;i<users.length;++i){

        index = participants.findIndex(x => x.id == users[i]);

    %>
    <tr>
        <td><%=i+1%>. </td>
        <td><%=participants[index].name%></td>
        <td><%=participants[index].email%></td>
        <td><%=participants[index].phone%></td>
        <td><%=participants[index].college%></td>
        <td><%=participants[index].degree%></td>
        <td><%=participants[index].branch%></td>
        <td><%=participants[index].year%></td>
        <td><%=referrers[i]%></td>
    </tr>
    <%}%>
    </tbody>
</table>


<% include adminPartials/footer.ejs %>