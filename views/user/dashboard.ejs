<body>
<link rel="stylesheet" href="/css/profile3.css">

<% include ../partials/new-nav-bar %>
<% include ../partials/login-modal %>


<div class="title-section" style="margin-top: 30px;">
    <h4 class="overlay-section" style="font-weight: bold; font-size: 2em;">Dashboard</h4>
    <h3 class="subtitle-section" style="font-size: 1.5em;">Your Infox. Your Events</h3>
</div>

<div class="container" style="width: 90%;">
    <div class="row">
        <!--<div class="col m8"></div>-->
        <div class="col m10 s6" style="text-align: right">
            <a href="/user/profile"><button class="btn #2196f3 blue">PROFILE</button></a>
        </div>
        <div class="col m2 s6">
            <a href="/logout"><button class="btn #d32f2f red darken-2" href="/logout">LOGOUT</button></a>
        </div>
    </div>
    <div class="events">

        <%
        console.log("events.length:" + events.length)
        for(i=0;i<events.length;++i){%>
        <div class="card">
            <div class="card-inner-wrapper">
                <div class="pic-wrapper">
                    <div class="pic">
                        <img src="<%=events[i].image.substr(8)%>">
                    </div>
                    <div class="pic-info">
                        <div class="event-info">
                            <div class="event-heading"><%=events[i].name%></div>
                            <div class="event-desc">
                                <%=events[i].short%>
                            </div>
                        </div>
                        <a href="/events"><div class="reg-btn prog" id="<%=events[i].id%>">View More</div></a>
                    </div>
                </div>
            </div>
        </div>
        <%}
        if(events.length === 0){
        %>
        <h1>No Registered Events Yet</h1>
        <%}%>
    </div>
</div>


<script type="text/javascript" src="/js/materialize.js"></script>
<script>
    $(document).ready(function(){
        $('select').formSelect();
    });

    $(document).ready(function(){
        $('.sidenav').sidenav();
    });

</script>
<% include ../partials/footer %>


<!-- Nav below is for hamburger -->

<% include ../partials/hamburger %>


<script src="/js/common.js"></script>
<script src="/js/team.js"></script>
</body>

