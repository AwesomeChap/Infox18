  <script>
    setTimeout(() => {
      update();
      var saga = document.getElementById("saga");
      var scene = new ParticleSaga.Scene(saga, targets, {
        numParticles: 40000,
        sort: ParticleSaga.VertexSort.leftToRight
      });
      scene.load(function () {
        // setupDemoUI();
        scene.setTarget(0);
        scene.startSlideshow();
      });
    }, 3500);


  </script>
  <script type="text/javascript">
    console.log('true');
    var t1 = new TimelineMax({ paused: true });
    t1.to(".one", 0.03, {
      y: 6,
      rotation: 45,
      ease: Expo.easeInOut
    });
    t1.to(".two", 0.05, {
      y: -6,
      rotation: -45,
      ease: Expo.easeInOut,
      delay: -0.3
    });
    t1.to(".menu", 1, {
      top: "0%",
      ease: Expo.easeInOut,
      delay: -1
    });
    t1.staggerFrom(".menu ul li", 0.5, { x: -200, opacity: 0, ease: Expo.easeOut }, 0.03);
    t1.reverse();
    $(document).on("click", ".toggle-btn", function () {
      t1.reversed(!t1.reversed());
    });
    $(document).on("click", "a", function () {
      t1.reversed(!t1.reversed());
    });



  </script>

