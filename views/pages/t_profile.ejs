            </div>
            <div class="grid-65">
              <input type="email" id="email" tabindex="6" />
            </div>
          </fieldset>
          <!-- Looking for Work -->
          <fieldset>
            <div class="grid-35">
              <label for="forHire">College</label>
            </div>
            <div class="grid-65">
              <select name="work" id="College" tabindex="7">
                <option selected="selected" value="--------------" disabled>--------------</option>
                <option value="yes">usict</option>
                <option value="no">dtu</option>
                <option value="no">nsit</option>
                <option value="no">bvp</option>
                <option value="no">iit delhi</option>
              </select>
            </div>
          </fieldset>
          <!-- Highest Qualification -->
          <fieldset>
            <div class="grid-35">
              <label for="qualification">Branch</label>
            </div>
            <div class="grid-65">
              <select name="qualification" id="branch" tabindex="8">
                <option selected="selected" value="--------------" disabled>--------------</option>
                <option value="primary_school">branch 1</option>
                <option value="primary_school">branch 2</option>
                <option value="primary_school">branch 3</option>
                <option value="primary_school">branch 4</option>
              </select>
            </div>
          </fieldset>
          <!-- year -->
          <fieldset>
            <div class="grid-35">
              <label for="qualification">year</label>
            </div>
            <div class="grid-65">
              <select name="qualification" id="year" tabindex="8">
                <option selected="selected" value="--------------" disabled>--------------</option>
                <option value="primary_school">1</option>
                <option value="primary_school">2</option>
                <option value="primary_school">3</option>
                <option value="primary_school">4</option>
              </select>
            </div>
          </fieldset>
